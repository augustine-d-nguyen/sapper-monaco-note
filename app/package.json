{
  "name": "beastmaster",
  "description": "the online note using monaco editor.",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build",
    "export": "sapper export",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "clean": "rimraf ../functions/__sapper__ && rimraf ../functions/static",
    "copy": "mkdirp ../functions/__sapper__/build && mkdirp ../functions/static && ncp ./__sapper__/build ../functions/__sapper__/build && ncp ./static ../functions/static",
    "deploy": "yarn build && yarn clean && yarn copy"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "firebase-admin": "^9.4.2",
    "md5": "^2.3.0",
    "monaco-editor": "^0.21.2",
    "node-fetch": "^2.6.1",
    "polka": "next",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/node": "^14.11.1",
    "@types/polka": "^0.5.1",
    "css-loader": "^5.0.1",
    "file-loader": "^6.0.0",
    "mkdirp": "^1.0.4",
    "monaco-editor-webpack-plugin": "^2.1.0",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "sapper": "^0.28.0",
    "style-loader": "^2.0.0",
    "svelte": "^3.17.3",
    "svelte-check": "^1.0.46",
    "svelte-loader": "^2.9.0",
    "svelte-preprocess": "^4.3.0",
    "ts-loader": "^8.0.4",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3",
    "webpack": "^4.7.0",
    "webpack-modules": "^1.0.0"
  }
}
